
<html>
<head>
    <title>RESTEndpoints</title>
</head>
    <body>
        <div id="links">
            <h2>Navigation</h2>
            <a href="/" rel="home">Home</a><br />
        </div>
        <h1>Welcome to Restful Endpoints!</h1>
        <div id="bugs">
            <ul class="all">
                @foreach (RestEndpoints.Core.Models.ContractDescriptor contract in @Model)
                {
                    <li>
                        <a href="/@contract.Endpoint/@contract.Name">@contract.Name</a>
                    </li>
                }
            </ul>
        </div>
    

    </body>
</html>